<h1>Edit your objective:</h1>

<%= form_for( @objective, :url => trip_objective_path ) do |f| %>

<%= render :partial => 'form_elements', :locals => { :f => f} %>

  <div class="actions">
    <%= f.submit "Submit" %>
  </div>
  <br>
<% end %>

<%= link_to 'Show',  trip_objective_path(@objective) %> | <%= link_to 'Back', trip_path(@objective.trip) %> |

<%= render 'layouts/links' %>